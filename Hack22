----- Game Services
Debris = game:GetService("Debris")
Players = game:GetService("Players")
CoreGui = game:GetService("CoreGui")
TweenService = game:GetService("TweenService")




------ Modules 
local Games = {
	_CurrentGame = nil;
	Sanctum = {
		ID = 9212627552;
		Moderators = {2817760,81919939}
	};
	NotRogueLineage = {
		ID = 9149676498;
		Moderators = {1935133078,466077984,668057530,56806120}
	};
}



------ Functions
function Alert()
	local Sound = Instance.new("Sound",CoreGui.GalaxyExploit2)
	Sound.SoundID = "rbxassetid://6043410483"
	Sound:Play()
	Debris:AddItem(Sound,2)
end
function CheckIfMod(PlayerToCheck)
	if table.find(Games[Games._CurrentGame].Moderators,PlayerToCheck.UserId) then
		return true
	else
		return false
	end
end
function ReportMod(Mod)
	local NotificationInfo = {Title="Moderator : "..Mod.Name,Text="Mod In The Server be careful",Duration=5} game.StarterGui:SetCore ("SendNotification", NotificationInfo)
end
function CheckForMods()
	for _,v in pairs(Players:GetChildren()) do
		if CheckIfMod(v) == true then
			ReportMod(v)
		end
	end
end
function CheckWhatGame()
	local _game = nil
	local ID = game.PlaceId
	if ID == Games.Sanctum.ID then
		_game = "Sanctum"
	elseif ID == Games.NotRogueLineage.ID then
		_game = "NotRogueLineage"
	else
		_game = "Non Supported Game"
	end
	return _game
end


--Rest of code
Exploiter = game.Players.LocalPlayer
ExploitersCharacter = Exploiter.Character
Games._CurrentGame = CheckWhatGame()
ScreenGui  = Instance.new("ScreenGui")
ScreenGui.Name = "GalaxyExploit2"
Frame = Instance.new("Frame",ScreenGui)
ScreenGui.Parent = game.Players.LocalPlayer.PlayerGui
Frame.Position = UDim2.new(0.431, 0,0.029, 0)
Frame.Size = UDim2.new(0, 300,0, 100)
Frame.BackgroundColor = BrickColor.Black()
GalaxySploitLabel = Instance.new("TextLabel",Frame)
GalaxySploitLabel.TextWrapped = true
GalaxySploitLabel.TextScaled = true
GalaxySploitLabel.BackgroundTransparency = 1
GalaxySploitLabel.Text = "Galaxy Hub Loaded For :"..CheckWhatGame()
GalaxySploitLabel.TextColor = BrickColor.White()
GalaxySploitLabel.Size = UDim2.new(0, 200,0, 32)
GalaxySploitLabel.Font = Enum.Font.Highway
task.wait(1)

DownTween = TweenService:Create(Frame,TweenInfo.new(1),{
	Position = UDim2.new(0.431, 0,0.445, 0)
})
DownTween:Play()
DownTween.Completed:Connect(function()
	TweenService:Create(GalaxySploitLabel,TweenInfo.new(1),{
		TextTransparency = 1
	}):Play()
	TweenService:Create(Frame,TweenInfo.new(1),{
		BackgroundTransparency = 1
	}):Play()
	Debris:AddItem(ScreenGui,2)
end)
Actions = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)
GridLayout = Instance.new("UIGridLayout",Actions)
FallDamageText = Instance.new("TextLabel",Actions)
FallDamageText.BackgroundTransparency = 1
FallDamageText.Text = "Fall Damage"
FallDamageTriggerButton = Instance.new("TextButton",Actions)
FallDamageTriggerButton.Text = "Off"
FallDamageTriggerButton.BackgroundTransparency = 1
FallDamageTriggerButton.MouseButton1Down:Connect(function()
	if FallDamageTriggerButton.Text == "Off" then
		FallDamageTriggerButton.Text = "On"
		ExploitersCharacter:FindFirstChild("CharacterHandler"):FindFirstChild("Remotes").Parent = ExploitersCharacter:FindFirstChild("CharacterHandler")
	elseif FallDamageTriggerButton == "On" then
		FallDamageTriggerButton.Text = "Off"
		ExploitersCharacter:FindFirstChild("CharacterHandler"):FindFirstChild("ApplyFallDamage").Parent = ExploitersCharacter:FindFirstChild("CharacterHandler"):FindFirstChild("Remotes")
	end
end)
Slider = Instance.new("Frame",Actions)
Slider.Size = UDim2.new(0, 208,0, 716)
Slider.Position = UDim2.new(0.07, 0,0.065, 0)
UICorner = Instance.new("UICorner",Slider)
UICorner.CornerRadius = UDim.new(0,8)
Players.ChildAdded:Connect(function(AddedPlayer)
	if CheckIfMod(AddedPlayer) == true then
		ReportMod(AddedPlayer)
	end
end)
task.wait(1)
CheckForMods()
